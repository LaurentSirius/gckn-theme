{
	"name": "gckn-dark",
	"type": "dark",
	"colors": {
		"editor.background": "#181a1b",
		"editor.foreground": "#cdcac2",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Opérateurs + mots clés",
			"scope": [
				"storage.type.js",
				"keyword.operator.assignment.js",
				"punctuation.separator.key-value.js",
				"keyword.operator.ternary.js",
				"storage.type.function.arrow.js",
				"storage.type.js",
				"keyword.operator.new.js",
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.end.js",
				"storage.modifier.async.js",
				"keyword.control.trycatch.js",
				"keyword.control.conditional.js",
				"storage.type.class.js",
				"storage.modifier.js",
				"keyword.control.flow.js",
				"keyword.operator.comparison.js",
				"keyword.operator.assignment.compound.js",
				"variable.language.this.js",
				"keyword.operator.logical.js",
				"support.type.object.module.js",
				"keyword.operator.relational.js"
			],
			"settings": {
				"foreground": "#df6470"
			}
		},
		{
			"name": "Types",
			"scope": [
				"constant.language.boolean.false.js",
				"constant.language.null.js",
				"constant.language.undefined.js",
				"constant.numeric.decimal.js",
				"constant.numeric.decimal.js",
				"punctuation.definition.string.begin.js",
				"punctuation.definition.string.end.js",
				"punctuation.definition.string.template.begin.js",
				"punctuation.definition.string.template.end.js"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff9a56"
			}
		},
		{
			"name": "Fonctions + classes",
			"scope": [
				"entity.name.function.js",
				"variable.language.super.js",
				"variable.parameter.js",
				"entity.name.type.js",
				"entity.name.type.class.js",
			],
			"settings": {
				"foreground": "#8777d3"
			}
		},
		{
			"name": "Commentaires",
			"scope": [
				"comment.line.double-slash.js",
				"comment.block.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5a5a5a"
			}
		},
		{
			"name": "Initialisation variables",
			"scope": [
				"variable.other.constant.js",
				"variable.parameter.js"
			],
			"settings": {
				"fontStyle": "italic bold underline",
				"foreground": "#ebe8e0"
			}
		},
		{
			"name": "Utilisation des variables",
			"scope": [
				"variable.other.object.js",
				"variable.other.readwrite.js",
				"entity.other.inherited-class.js"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#ebe8e0"
			}
		}
	]
}