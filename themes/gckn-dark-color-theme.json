	"tokenColors": [
		{
			"name": "Opérateurs + mots clés JavaScript",
			"scope": [
				"storage.type.js",
				"storage.type.class.js",
				"storage.modifier.js",
				"punctuation.separator.key-value.js",
				"keyword.operator.assignment.js",
				"storage.type.function.arrow.js",
				"keyword.operator.logical.js",
				"keyword.control.flow.js",
				"keyword.operator.comparison.js",
				"keyword.operator.spread.js",
				"storage.modifier.async.js",
				"keyword.operator.assignment.compound.js",
				"keyword.operator.arithmetic.js",
				"keyword.control.conditional.js",
				"keyword.operator.relational.js",
				"keyword.control.trycatch.js",
				"keyword.operator.rest.js",
				"keyword.operator.new.js"
			],
			"settings": {
				"foreground": "#df6470"
			}
		},
		{
			"name": "Types + support fonction/class JavaScript",
			"scope": [
				"variable.other.constant.js",
				"support.function.js",
				"support.type.object.module.js",
				"support.class.promise.js",
				"variable.other.property.js",
				"support.function.console.js",
				"support.variable.object.process.js",
				"variable.language.super.js",
				"constant.language.boolean.true.js",
				"variable.language.this.js",
				"variable.other.constant.property.js",
				"constant.numeric.decimal.js",
				"constant.language.null.js",
				"support.class.builtin.js"
			],
			"settings": {
				"foreground": "#4eaaff"
			}
		},
		{
			"name": "Fonctions + classes JavaScript",
			"scope": [
				"entity.name.function.js",
				"entity.name.type.module.js",
				"entity.name.type.class.js",
				"entity.other.inherited-class.js",
				"entity.name.type.js",
				"support.class.console.js"
			],
			"settings": {
				"foreground": "#9878d3"
			}
		},
		{
			"name": "Commentaires JavaScript",
			"scope": [
				"comment.line.double-slash.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5a5a5a"
			}
		},
		{
			"name": "Commentaires GitIgnore",
			"scope": [
				"comment.line.number-sign.ignore"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5a5a5a"
			}
		},
		{
			"name": "Ponctuation Markdown",
			"scope": [
				"punctuation.definition.heading.markdown",
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.quote.begin.markdown",
				"punctuation.definition.markdown",
				"punctuation.definition.string.begin.markdown",
				"string.other.link.description.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#df6470"
			}
		},
		{
			"name": "Titre + liens Markdown",
			"scope": [
				"entity.name.section.markdown",
				"string.other.link.title.markdown",
				"markup.underline.link.markdown"
			],
			"settings": {
				"foreground": "#4eaaff"
			}
		},
		{
			"name": "Fonctions + classes Markdown",
			"scope": [
				"markup.bold.markdown",
				"markup.inline.raw.string.markdown",
				"markup.italic.markdown"
			],
			"settings": {
				"foreground": "#9878d3"
			}
		},
		{
			"name": "Commentaires Markdown",
			"scope": [
				"comment.block.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5a5a5a"
			}
		},
		{
			"name": "Strings JSON",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#9878d3"
			}
		},
		{
			"name": "Ponctuation JSON",
			"scope": [
				"punctuation.separator.dictionary.key-value.json"
			],
			"settings": {
				"foreground": "#df6470"
			}
		},
	]
